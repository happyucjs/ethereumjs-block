  ],
  "author": "IrChain Team (contact@irchain.io)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/icjs/icjs-block/issues"
  },
  "homepage": "https://github.com/icjs/icjs-block#readme",
  "dependencies": {
    "async": "^2.0.1",
    "icjs-common": "^0.0.1",
    "icjs-tx": "git+https://github.com/icjs/icjs-tx.git",
    "icjs-util": "^0.0.1",
    "merkle-patricia-tree": "^2.1.2"
  },
  "devDependencies": {
    "babelify": "^8.0.0",
    "babel-preset-env": "^1.6.1",
    "browserify": "^15.0.0",
    "coveralls": "^2.11.6",
    "documentation": "8.1.0",
    "istanbul": "^0.4.2",
    "standard": "^8.4.0",
    "tape": "^4.2.0"
